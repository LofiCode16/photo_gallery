<h1>New photo</h1>

<%= form_with(model: @photo, local: true) do |f| %>

    <div>
        <%= f.label :image %>
        <%= f.text_field :image_url %>
    </div>

    <div>
        <%= f.label :title %>
        <%= f.text_field :title %>
    </div>

    <div>
        <%= f.submit %>
    </div>
<% end %>